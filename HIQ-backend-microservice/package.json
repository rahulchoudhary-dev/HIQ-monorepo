{
  "name": "hiq-backend-microservice",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "start:dev": "dotenv -e .env.development -- nodemon index.js",
    "start:prod": "dotenv -e .env.production -- node index.js",
    "migration:generate": "dotenv -e .env.development -- npx sequelize-cli migration:generate --name",
    "db:migrate:dev": "dotenv -e .env.development -- sequelize-cli db:migrate --env development",
    "db:migrate:prod": "dotenv -e .env.production -- sequelize-cli db:migrate --env production",
    "db:migrate:undo:dev": "dotenv -e .env.development -- sequelize-cli db:migrate:undo --env development",
    "db:migrate:undo:prod": "dotenv -e .env.production -- sequelize-cli db:migrate:undo --env production",
    "db:seed:dev": "dotenv -e .env.development -- sequelize-cli db:seed:all --env development",
    "db:seed:prod": "dotenv -e .env.production -- sequelize-cli db:seed:all --env production",
    "deploy:prod:ec2:ecosystem": "dotenv -e .env.production -- sequelize-cli db:migrate --env production && pm2 start ecosystem.config.js --env production",
    "deploy:safe:ec2:prod": "dotenv -e .env.production -- bash scripts/deploy-safe-ec2.sh",
    "deploy:ecs:prod": "bash scripts/deploy-ecs.sh"
  },
  "author": "Rahul Choudhary",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.916.0",
    "@aws-sdk/lib-storage": "^3.916.0",
    "@aws-sdk/s3-request-presigner": "^3.918.0",
    "@bull-board/api": "^6.14.0",
    "@bull-board/express": "^6.14.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.12.2",
    "bullmq": "^5.61.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^7.4.4",
    "express": "^5.1.0",
    "express-basic-auth": "^1.2.1",
    "format": "^0.2.2",
    "ioredis": "^5.8.2",
    "multer": "^2.0.2",
    "multer-s3": "^3.0.1",
    "nodemon": "^3.1.10",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "prettier": "^3.6.2",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3"
  }
}
